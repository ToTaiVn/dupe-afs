function Return_Lowest_Unit(Unit_Table)
    local Lowest_Level, Lowest_Unit = math.huge
    
    for _, Unit in ipairs(Unit_Table) do
            if Unit["Level"] < Lowest_Level then
                Lowest_Level = Unit["Level"]
                Lowest_Unit = Unit
            end
    end
    
    return Lowest_Unit
end

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "BingBong Hub DUPE AFS", HidePremium = false, SaveConfig = false, IntroText = "BingBongHub", IntroEnabled = true, ConfigFolder = "BingBongHub"})

local MainTab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

MainTab:AddDropdown({
	Name = "Type",
	Default = "1",
	Options = "Start Dataloss",
	Callback = function(Value)
		getgenv().Type = Value
	end    
})

MainTab:AddButton({
	Name = "Start Data Loss",
	Callback = function()
      		if Type == "Start Dataloss" then
        local args = {
            [1] = Return_Lowest_Unit(require(game.ReplicatedStorage.ModuleScripts.LocalDairebStore).GetStoreProxy("GameData"):GetData("Pets")).UID,
            [2] = true,
            [3] = "\191"
        }

        game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("PetVault"):FireServer(unpack(args))

        MainTab:AddLabel("Data Loss Started")
        OrionLib:MakeNotification({
	Name = "Data Loss Started Please wait when this notification run out of ",
	Content = "Thanks For Use Us Script",
	Image = "rbxassetid://4483345998",
	Time = 5
})
		end
    end    
})

local DiscordTab = Window:MakeTab({
	Name = "Discord",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

DiscordTab:AddLabel("https://discord.gg/PsbyeRmU")

DiscordTab:AddButton({
	Name = "Copy Link Discord",
	Callback = function()
      		setclipboard("https://discord.gg/PsbyeRmU")
      OrionLib:MakeNotification({
	Name = "Copied Link",
	Content = "Hi....",
	Image = "rbxassetid://4483345998",
	Time = 5
})
  	end
  })
OrionLib:Init()

